      },
      "error": {
        "50": "#fef2f2",
        "500": "#ef4444",
        "700": "#b91c1c"
      }
    },
    "typography": {
      "fontFamily": {
        "sans": ["Inter", "system-ui", "-apple-system", "sans-serif"],
        "mono": ["Fira Code", "monospace"]
      },
      "fontSize": {
        "xs": "0.75rem",
        "sm": "0.875rem",
        "base": "1rem",
        "lg": "1.125rem",
        "xl": "1.25rem",
        "2xl": "1.5rem",
        "3xl": "1.875rem",
        "4xl": "2.25rem"
      },
      "fontWeight": {
        "normal": "400",
        "medium": "500",
        "semibold": "600",
        "bold": "700"
      },
      "lineHeight": {
        "tight": "1.25",
        "normal": "1.5",
        "relaxed": "1.75"
      }
    },
    "spacing": {
      "0": "0",
      "1": "0.25rem",
      "2": "0.5rem",
      "3": "0.75rem",
      "4": "1rem",
      "5": "1.25rem",
      "6": "1.5rem",
      "8": "2rem",
      "10": "2.5rem",
      "12": "3rem",
      "16": "4rem",
      "20": "5rem",
      "24": "6rem"
    },
    "borderRadius": {
      "none": "0",
      "sm": "0.125rem",
      "md": "0.375rem",
      "lg": "0.5rem",
      "xl": "0.75rem",
      "2xl": "1rem",
      "full": "9999px"
    },
    "shadows": {
      "sm": "0 1px 2px 0 rgb(0 0 0 / 0.05)",
      "md": "0 4px 6px -1px rgb(0 0 0 / 0.1), 0 2px 4px -2px rgb(0 0 0 / 0.1)",
      "lg": "0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1)",
      "xl": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)"
    }
  },
  "components": {
    "button": {
      "primary": {
        "backgroundColor": "#0ea5e9",
        "color": "#ffffff",
        "padding": "0.75rem 1.5rem",
        "borderRadius": "0.5rem",
        "fontSize": "1rem",
        "fontWeight": "600",
        "boxShadow": "0 4px 6px -1px rgb(0 0 0 / 0.1)",
        "hover": {
          "backgroundColor": "#0284c7",
          "transform": "translateY(-1px)",
          "boxShadow": "0 10px 15px -3px rgb(0 0 0 / 0.1)"
        },
        "active": {
          "transform": "translateY(0)"
        },
        "focus": {
          "outline": "2px solid #0ea5e9",
          "outlineOffset": "2px"
        }
      },
      "secondary": {
        "backgroundColor": "transparent",
        "color": "#0ea5e9",
        "border": "1px solid #0ea5e9",
        "padding": "0.75rem 1.5rem",
        "borderRadius": "0.5rem",
        "fontSize": "1rem",
        "fontWeight": "600",
        "hover": {
          "backgroundColor": "#f0f9ff",
          "transform": "translateY(-1px)"
        }
      },
      "ghost": {
        "backgroundColor": "transparent",
        "color": "#6b7280",
        "padding": "0.5rem 1rem",
        "borderRadius": "0.375rem",
        "fontSize": "0.875rem",
        "fontWeight": "500",
        "hover": {
          "backgroundColor": "#f3f4f6",
          "color": "#374151"
        }
      }
    },
    "card": {
      "backgroundColor": "#ffffff",
      "borderRadius": "0.75rem",
      "padding": "1.5rem",
      "boxShadow": "0 4px 6px -1px rgb(0 0 0 / 0.1)",
      "border": "1px solid #e5e7eb",
      "hover": {
        "transform": "translateY(-2px)",
        "boxShadow": "0 10px 15px -3px rgb(0 0 0 / 0.1)"
      }
    },
    "input": {
      "backgroundColor": "#ffffff",
      "border": "1px solid #d1d5db",
      "borderRadius": "0.5rem",
      "padding": "0.75rem 1rem",
      "fontSize": "1rem",
      "focus": {
        "borderColor": "#0ea5e9",
        "outline": "none",
        "boxShadow": "0 0 0 3px rgb(14 165 233 / 0.1)"
      },
      "error": {
        "borderColor": "#ef4444",
        "boxShadow": "0 0 0 3px rgb(239 68 68 / 0.1)"
      }
    },
    "quickActionsDock": {
      "backgroundColor": "#ffffff",
      "borderRadius": "1rem",
      "padding": "0.75rem",
      "boxShadow": "0 20px 25px -5px rgb(0 0 0 / 0.1), 0 8px 10px -6px rgb(0 0 0 / 0.1)",
      "border": "1px solid #e5e7eb",
      "backdropBlur": "12px",
      "position": "fixed",
      "bottom": "1rem",
      "left": "50%",
      "transform": "translateX(-50%)",
      "zIndex": "100"
    },
    "miniCart": {
      "backgroundColor": "#ffffff",
      "borderRadius": "0.75rem",
      "padding": "1rem",
      "boxShadow": "0 20px 25px -5px rgb(0 0 0 / 0.1)",
      "border": "1px solid #e5e7eb",
      "position": "fixed",
      "top": "4rem",
      "right": "1rem",
      "width": "320px",
      "maxHeight": "400px",
      "zIndex": "50"
    }
  },
  "animations": {
    "fadeIn": {
      "keyframes": "0% { opacity: 0; } 100% { opacity: 1; }",
      "duration": "150ms",
      "easing": "ease-out"
    },
    "slideUp": {
      "keyframes": "0% { transform: translateY(10px); opacity: 0; } 100% { transform: translateY(0); opacity: 1; }",
      "duration": "200ms",
      "easing": "ease-out"
    },
    "scaleIn": {
      "keyframes": "0% { transform: scale(0.95); opacity: 0; } 100% { transform: scale(1); opacity: 1; }",
      "duration": "150ms",
      "easing": "ease-out"
    },
    "pulse": {
      "keyframes": "0%, 100% { opacity: 1; } 50% { opacity: 0.5; }",
      "duration": "1000ms",
      "easing": "ease-in-out",
      "iteration": "infinite"
    },
    "bounce": {
      "keyframes": "0%, 100% { transform: translateY(0); } 50% { transform: translateY(-5px); }",
      "duration": "500ms",
      "easing": "ease-in-out"
    }
  },
  "breakpoints": {
    "xs": "320px",
    "sm": "640px",
    "md": "768px",
    "lg": "1024px",
    "xl": "1280px",
    "2xl": "1536px"
  },
  "zIndex": {
    "overlay": "40",
    "dropdown": "50",
    "modal": "60",
    "toast": "70",
    "tooltip": "80",
    "dock": "100"
  }
}
