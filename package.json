{
  "name": "sass-store",
  "version": "1.0.0",
  "description": "Multitenant SaaS platform for beauty salons with 10/10 UX",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:cloudflare": "cross-env CF_PAGES=1 turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "deploy": "npm run build:cloudflare && echo 'Build complete! Deploy to Cloudflare Pages via dashboard or wrangler'",
    "deploy:preview": "npm run build:cloudflare && wrangler pages deploy apps/web/out --branch=preview",
    "deploy:production": "npm run build:cloudflare && wrangler pages deploy apps/web/out --branch=main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:security": "vitest run tests/security",
    "test:e2e:subset": "ts-node ./tests/e2e/subset/*.e2e.ts",
    "typecheck": "turbo run typecheck",
    "db:generate": "drizzle-kit generate",
    "db:introspect": "drizzle-kit introspect",
    "db:push": "drizzle-kit push --config ./drizzle.config.ts",
    "autoresume": "ts-node ./tools/autoresume.ts",
    "autoresume:daemon": "ts-node ./tools/autoresume-daemon.ts",
    "autoresume:start": "ts-node ./tools/autoresume-daemon.ts",
    "workflow:status": "ts-node --transpile-only ./tools/workflow-status.ts",
    "workflow:cleanup": "ts-node --transpile-only ./tools/workflow-cleanup.ts",
    "swarm:status": "ts-node --transpile-only ./agents/swarm/cli/status.ts",
    "swarm:start": "npx ts-node --transpile-only ./agents/swarm/cli/start.ts",
    "swarm:resume": "ts-node --transpile-only ./agents/swarm/cli/resume.ts",
    "swarm:continue": "ts-node --transpile-only ./agents/swarm/cli/continue.ts",
    "lint:imports": "ts-node ./tests/integration/lint-paths.int.spec.ts",
    "seo:analyze": "ts-node -e \"console.log('Running SEO analysis...')\"",
    "a11y:audit": "ts-node -e \"console.log('Running accessibility audit...')\"",
    "perf:analyze": "ts-node -e \"console.log('Running performance analysis...')\"",
    "security:full": "npm run swarm:start \"security scan full\"",
    "security:quick": "ts-node ./scripts/security-quick-scan.ts",
    "security:autofix": "npx ts-node ./scripts/security-autofix-simple.ts",
    "security:check-deps": "npm audit --audit-level=high",
    "security:update-deps": "npm audit fix && npm update",
    "rls:apply": "npx ts-node ./scripts/apply-rls.ts",
    "rls:test": "npx ts-node ./scripts/test-rls.ts",
    "video:migrate": "npx ts-node ./scripts/migrate-video-processing.ts",
    "video:test": "cd apps/api && npx ts-node ../scripts/test-video-processing.ts",
    "video:worker": "cd scripts && python video-processor-worker.py",
    "video:install-deps": "cd scripts && pip install -r requirements.txt",
    "test:e2e:all": "playwright test",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:firefox": "playwright test --project=firefox",
    "test:e2e:webkit": "playwright test --project=webkit",
    "test:e2e:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:3001",
    "prepare": "husky || true",
    "postinstall": "node -e \"console.log('âœ“ Dependencies installed with React', require('./node_modules/react/package.json').version)\""
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.0",
    "@turbo/gen": "^1.10.12",
    "@types/jest": "^30.0.0",
    "@types/minimatch": "^6.0.0",
    "@types/node": "20.8.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.21",
    "critters": "^0.0.20",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.12.8",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "tailwindcss": "^4.1.14",
    "turbo": "^1.10.12",
    "typescript": "^5.2.2",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@alloc/quick-lru": "^5.2.0",
    "@apollo/server": "^5.0.0",
    "@as-integrations/next": "^4.0.0",
    "@aws-sdk/client-s3": "^3.908.0",
    "@aws-sdk/lib-storage": "^3.908.0",
    "@aws-sdk/s3-request-presigner": "^3.908.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/pg": "^8.15.5",
    "@upstash/redis": "^1.35.4",
    "drizzle-orm": "^0.31.0",
    "framer-motion": "^12.23.22",
    "gsap": "^3.13.0",
    "next": "14.2.35",
    "next-auth": "5.0.0-beta.30",
    "pg": "^8.16.3",
    "postgres": "^3.4.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "resend": "^6.1.2",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76"
  }
}
